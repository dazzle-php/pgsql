{
    "name": "dazzle-php/pgsql",
    "description": "Dazzle Asynchronous PgSQL.",
    "keywords": [
        "dazzle", "dazzle-php", "async", "sql", "pgsql", "storage", "database", "postgres"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dazzle-php/pgsql/issues",
        "source": "https://github.com/dazzle-php/pgsql"
    },
    "authors": [
        {
            "name":     "Kamil Jamroz",
            "homepage": "https://github.com/khelle"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/dazzle-php/pgsql/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.7",
        "dazzle-php/loop": "^0.5.0",
        "dazzle-php/event": "^0.5.0",
        "dazzle-php/promise": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8.0 <5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\PgSQL\\": "src/PgSQL",
            "Dazzle\\PgSQL\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
